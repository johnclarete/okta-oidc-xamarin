<?xml version="1.0" encoding="utf-8"?>
<manifest
        xmlns:android="http://schemas.android.com/apk/res/android"
        package="Mono.Android.DebugRuntime"
        android:installLocation="auto"
        android:versionCode="1580759142"
        android:versionName="10.3.0-0">
    <uses-sdk android:minSdkVersion="4" android:targetSdkVersion="16" /> 
    <application android:label="@string/app_name">
        <receiver android:name="com.xamarin.mono.android.ExternalStorageDirectory">
            <intent-filter>
                <action android:name="mono.android.intent.action.EXTERNAL_STORAGE_DIRECTORY" />
            </intent-filter>
        </receiver>
        <receiver android:name="com.xamarin.mono.android.PackageVersions">
            <intent-filter>
                <action android:name="mono.android.intent.action.PACKAGE_VERSIONS" />
            </intent-filter>
        </receiver>
    </application>
</manifest>
