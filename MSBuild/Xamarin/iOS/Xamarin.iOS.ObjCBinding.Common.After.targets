<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<Import Project="$(MSBuildThisFileDirectory)Xamarin.iOS.Windows.After.targets" Condition="Exists('$(MSBuildThisFileDirectory)Xamarin.iOS.Windows.After.targets')" />
	<Target Name="_SetRemotePropertyValues" AfterTargets="_CollectBundleResources">
		<GetPropertyValue SessionId="$(BuildSessionId)" FileName="$(MSBuildThisFileDirectory)Xamarin.iOS.ObjCBinding.CSharp.props" PropertyName="BTouchToolPath" TargetFrameworkIdentifier="$(TargetFrameworkIdentifier)">
			<Output TaskParameter="PropertyValue" PropertyName="BTouchToolPath" />
		</GetPropertyValue>
		<GetPropertyValue SessionId="$(BuildSessionId)" FileName="$(MSBuildThisFileDirectory)Xamarin.iOS.ObjCBinding.CSharp.props" PropertyName="BTouchToolExe" TargetFrameworkIdentifier="$(TargetFrameworkIdentifier)">
			<Output TaskParameter="PropertyValue" PropertyName="BTouchToolExe" />
		</GetPropertyValue>
		<GetPropertyValue SessionId="$(BuildSessionId)" FileName="$(MSBuildThisFileDirectory)Xamarin.iOS.ObjCBinding.CSharp.props" PropertyName="BaseLibDllPath" TargetFrameworkIdentifier="$(TargetFrameworkIdentifier)">
			<Output TaskParameter="PropertyValue" PropertyName="BaseLibDllPath" />
		</GetPropertyValue>
	</Target>
</Project>